        <column name="generalNote" order="240" numcolumns="4">
            <widget type="text" fieldname="generalNote" rows="1"/>
        </column>
        <column name="profesionalNote" order="250" numcolumns="4">
            <widget type="text" fieldname="profesionalNote" rows="1"/>
        </column>
    </columns>
</view>
